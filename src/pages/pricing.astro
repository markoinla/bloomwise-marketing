import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';
import Pricing2 from '@/components/blocks/pricing2';
import { Pricing1 } from '@/components/blocks/pricing1';
---
<DefaultLayout title={SITE_TITLE} description={SITE_DESCRIPTION}>
  <Background>
    <Pricing1 client:only='react' className="py-28 text-center lg:pt-44 lg:pb-32" />
    <Pricing2 />
  </Background>
</DefaultLayout>
